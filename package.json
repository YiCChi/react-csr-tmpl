{
  "name": "pure-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --host",
    "build-staging": "tsc && vite build --mode staging",
    "build-prod": "tsc && vite build",
    "preview": "tsc && vite build && vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "type": "tsc --incremental",
    "lint": "eslint --cache **/*.ts{,x}",
    "lint-staged": "lint-staged",
    "pre-commit": "pnpm run \"/(lint|type|lint-staged)/\"",
    "ci": "biome ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YiCChi/pure-react.git"
  },
  "keywords": [
    "react",
    "ts"
  ],
  "author": "syanxk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YiCChi/pure-react/issues"
  },
  "homepage": "https://github.com/YiCChi/pure-react#readme",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/system": "^5.15.6",
    "@mui/x-data-grid": "^6.19.2",
    "@mui/x-date-pickers": "^6.19.2",
    "lodash": "^4.17.21",
    "luxon": "3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.0",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prop-types": "15.8.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
