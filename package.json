{
  "name": "pure-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --host",
    "build-staging": "tsc && vite build --mode staging",
    "build-prod": "tsc && vite build",
    "preview": "tsc && vite build && vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "type": "tsc --incremental",
    "lint": "eslint --cache **/*.ts{,x}",
    "lint-staged": "lint-staged",
    "pre-commit": "pnpm run \"/(lint|type|lint-staged)/\"",
    "ci": "biome ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YiCChi/pure-react.git"
  },
  "keywords": [
    "react",
    "ts"
  ],
  "author": "syanxk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YiCChi/pure-react/issues"
  },
  "homepage": "https://github.com/YiCChi/pure-react#readme",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/system": "^5.14.18",
    "@mui/x-data-grid": "^6.18.1",
    "@mui/x-date-pickers": "^6.18.1",
    "lodash": "^4.17.21",
    "luxon": "3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@testing-library/react": "^14.1.0",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "@vitest/coverage-v8": "^1.0.0-beta.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prop-types": "15.8.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "vitest": "^1.0.0-beta.4"
  }
}
