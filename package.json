{
  "name": "pure-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite --host",
    "build-staging": "tsc && vite build --mode staging",
    "build-prod": "tsc && vite build",
    "preview": "tsc && vite build && vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "type": "tsc --incremental",
    "lint": "eslint --cache **/*.ts{,x}",
    "ci": "biome ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YiCChi/pure-react.git"
  },
  "keywords": [
    "react",
    "ts"
  ],
  "author": "syanxk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YiCChi/pure-react/issues"
  },
  "homepage": "https://github.com/YiCChi/pure-react#readme",
  "dependencies": {
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/system": "^5.15.15",
    "@mui/x-data-grid": "^7.2.0",
    "@mui/x-date-pickers": "^7.2.0",
    "lodash": "^4.17.21",
    "luxon": "3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@testing-library/react": "^15.0.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
