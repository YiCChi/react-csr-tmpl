{
  "name": "pure-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite --host",
    "build-staging": "tsc && vite build --mode staging",
    "build-prod": "tsc && vite build",
    "preview": "tsc && vite build && vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "type": "tsc --incremental",
    "lint": "eslint --cache **/*.ts{,x}",
    "ci": "biome ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YiCChi/pure-react.git"
  },
  "keywords": [
    "react",
    "ts"
  ],
  "author": "syanxk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YiCChi/pure-react/issues"
  },
  "homepage": "https://github.com/YiCChi/pure-react#readme",
  "dependencies": {
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/system": "^5.15.20",
    "@mui/x-data-grid": "^7.7.0",
    "@mui/x-date-pickers": "^7.7.0",
    "lodash": "^4.17.21",
    "luxon": "3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.2",
    "@testing-library/react": "^16.0.0",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
